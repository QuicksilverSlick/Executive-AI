{
  "name": "executive-ai-training",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build && npm run optimize && npm run postbuild",
    "postbuild": "node scripts/inject-polyfill-complete.js",
    "build:prod": "npm run build && npm run analyze",
    "preview": "astro preview",
    "astro": "astro",
    "optimize": "node scripts/optimize-build.js",
    "analyze": "astro build --analyze",
    "clean": "rm -rf dist .astro node_modules/.cache",
    "lighthouse": "npx lighthouse https://localhost:4321 --view",
    "verify-openai": "node scripts/verify-openai-setup.js",
    "test-token": "node scripts/test-token-endpoint.js",
    "setup-check": "npm run verify-openai && echo '\\nðŸ”§ Run npm run test-token to test the local endpoint'",
    "setup-complete": "node scripts/setup-complete.js",
    "setup-secure-env": "node scripts/setup-secure-env.js",
    "validate-api-keys": "node scripts/api-key-validator.js",
    "rotate-keys": "node scripts/rotate-api-keys.js",
    "monitor-api-usage": "node scripts/monitor-api-usage.js",
    "monitor-watch": "node scripts/monitor-api-usage.js --watch",
    "audit-client-security": "node scripts/audit-client-security.js",
    "security-full-audit": "npm run audit-client-security && npm run validate-api-keys",
    "clear-rate-limits": "node scripts/clear-rate-limits.js",
    "rate-limit-status": "node scripts/clear-rate-limits.js status",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:voice": "vitest run tests/integration/voice-agent-comprehensive.test.js",
    "test:audio": "vitest run src/tests/audio/",
    "test:audio-integration": "vitest run src/tests/integration/voice-agent-audio.test.ts",
    "test:audio-performance": "vitest run src/tests/performance/audio-performance.test.ts",
    "test:e2e": "playwright test tests/e2e/voice-agent-journey.e2e.test.js",
    "test:e2e:audio": "playwright test src/tests/e2e/voice-search-audio.e2e.ts",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test:run && npm run test:audio && npm run test:e2e",
    "test:audio-all": "npm run test:audio && npm run test:audio-integration && npm run test:audio-performance && npm run test:e2e:audio",
    "search-server": "node search-server.js",
    "search-api:dev": "node search-server.js",
    "cf:preview": "wrangler pages dev ./dist",
    "cf:deploy": "npm run build && wrangler pages deploy ./dist",
    "cf:deploy:preview": "npm run build && wrangler pages deploy ./dist --branch preview"
  },
  "dependencies": {
    "@astrojs/react": "^4.3.0",
    "@astrojs/sitemap": "^3.4.2",
    "@iconify-json/lucide": "^1.2.55",
    "@iconify-json/mdi": "^1.2.3",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "astro": "^5.12.9",
    "astro-compress": "^2.3.8",
    "astro-icon": "^1.1.5",
    "framer-motion": "^12.23.0",
    "lightningcss": "^1.30.1",
    "openai": "^4.67.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "web-vitals": "^5.0.3",
    "webrtc-adapter": "^8.2.3"
  },
  "devDependencies": {
    "@astrojs/cloudflare": "^12.6.3",
    "@astrojs/tailwind": "^6.0.2",
    "@playwright/test": "^1.40.0",
    "@vitest/ui": "^1.0.0",
    "jsdom": "^23.0.0",
    "tailwindcss": "^3.4.1",
    "vitest": "^1.0.0",
    "wrangler": "^4.30.0"
  }
}
